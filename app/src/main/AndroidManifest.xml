<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="friends.eevee">

    <application
        android:allowBackup="true"
        android:icon="@drawable/eevee48"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.TimeWall.TimeWall" />
        <activity android:name=".Activities.Scratch">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.TouchEvent.TouchEvent"></activity>
    </application>

</manifest>